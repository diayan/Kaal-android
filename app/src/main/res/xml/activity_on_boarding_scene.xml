<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <KeyFrameSet></KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/backgroundViewFirst"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/backgroundViewSecond"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:alpha="0"
            android:rotation="90"
            android:scaleX="0.2"
            android:scaleY="0.2"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.15" />

        <Constraint
            android:id="@+id/backgroundViewThird"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:alpha="0"
            android:rotation="90"
            android:scaleX="0.2"
            android:scaleY="0.2"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.15" />

        <Constraint
            android:id="@+id/independenceImageView"
            android:layout_width="100dp"
            android:layout_height="100dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/titleViewFirst"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="32dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/backgroundViewFirst"
            motion:layout_constraintVertical_bias="0.3"/>
        <Constraint
            android:id="@+id/subtitleViewFirst"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/titleViewFirst" />

        <Constraint
            android:id="@+id/cultureImageView"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:alpha="0"
            android:rotation="360"
            android:scaleX="0.3"
            android:scaleY="0.3"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/titleViewSecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="32dp"
            android:alpha="0"
            android:scaleX="0.4"
            android:scaleY="0.4"
            motion:layout_constrainedWidth="true"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/backgroundViewSecond"
            motion:layout_constraintVertical_bias="0.7" />

        <Constraint
            android:id="@+id/subTitleViewSecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="32dp"
            android:alpha="0"
            android:scaleX="0.4"
            android:scaleY="0.4"
            motion:layout_constrainedWidth="true"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="parent" />

        <Constraint
            android:id="@+id/flagImageView"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:alpha="0"
            android:rotation="180"
            android:scaleX="0.3"
            android:scaleY="0.3"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/titleViewThird"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:scaleX="0.4"
            android:scaleY="0.4"
            motion:layout_constrainedWidth="true"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/backgroundViewThird"
            motion:layout_constraintVertical_bias="0.7" />

        <Constraint
            android:id="@+id/subTitleViewThird"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/larger_size"
            android:layout_marginTop="@dimen/small_size"
            android:layout_marginEnd="@dimen/larger_size"
            android:alpha="0"
            android:scaleX="0.4"
            android:scaleY="0.4"
            motion:layout_constrainedWidth="true"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="parent" />

        <Constraint
            android:id="@+id/indicatorViewFirst"
            android:layout_width="@dimen/common_size"
            android:layout_height="@dimen/small_size"
            android:layout_marginBottom="@dimen/common_size"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@+id/indicatorViewSecond"
            motion:layout_constraintHorizontal_chainStyle="packed"
            motion:layout_constraintStart_toStartOf="parent" />

        <Constraint
            android:id="@+id/indicatorViewSecond"
            android:layout_width="@dimen/common_size"
            android:layout_height="@dimen/small_size"
            android:layout_marginStart="@dimen/common_size"
            android:layout_marginEnd="@dimen/common_size"
            motion:layout_constraintBottom_toBottomOf="@+id/indicatorViewFirst"
            motion:layout_constraintEnd_toStartOf="@+id/indicatorViewThird"
            motion:layout_constraintStart_toEndOf="@+id/indicatorViewFirst"
            motion:layout_constraintTop_toTopOf="@+id/indicatorViewFirst" />
        <Constraint
            android:id="@+id/indicatorViewThird"
            android:layout_width="@dimen/common_size"
            android:layout_height="@dimen/small_size"
            motion:layout_constraintBottom_toBottomOf="@+id/indicatorViewSecond"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/indicatorViewSecond"
            motion:layout_constraintTop_toTopOf="@+id/indicatorViewSecond" />
        <Constraint
            android:id="@+id/indicatorViewActiveTop"
            android:layout_width="@dimen/common_size"
            android:layout_height="@dimen/tiny_size"
            motion:layout_constraintEnd_toEndOf="@+id/indicatorViewFirst"
            motion:layout_constraintStart_toStartOf="@+id/indicatorViewFirst"
            motion:layout_constraintTop_toTopOf="@+id/indicatorViewFirst" />
        <Constraint
            android:id="@+id/indicatorViewActiveBottom"
            android:layout_width="@dimen/common_size"
            android:layout_height="@dimen/tiny_size"
            motion:layout_constraintBottom_toBottomOf="@+id/indicatorViewFirst"
            motion:layout_constraintEnd_toEndOf="@+id/indicatorViewFirst"
            motion:layout_constraintStart_toStartOf="@+id/indicatorViewFirst" />
        <Constraint
            android:id="@+id/finishButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:scaleX="0.2"
            android:scaleY="0.2"
            android:textColor="@android:color/white"
            motion:layout_constrainedWidth="true"
            motion:layout_constraintBottom_toTopOf="@id/indicatorViewFirst"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/subTitleViewThird" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

    </ConstraintSet>
</MotionScene>